#: ../gus-api-nip24pl.php:298
msgid "VAT ID"
msgstr "NIP"

#: ../gus-api-nip24pl.php:213
msgid "Fetch data"
msgstr "Pobierz dane"

#: ../gus-api-nip24pl.php:219
msgid " is invalid"
msgstr "jest nieprawidłowy."

#: ../gus-api-nip24pl.php:223
msgid ""
"Data outdated? <a href=\"javascript:nip24GetInvoiceData(true);\">Click here!"
"</a>"
msgstr ""
"Dane nieaktualne? <a href=\"javascript:nip24GetInvoiceData(true);\">Kliknij "
"tutaj!</a>"

